        </columns>
      </TableView>
      <ComboBox fx:id="pilihLokasiCB" layoutX="6.0" layoutY="8.0" onAction="#btnPilihLokasiCB" prefWidth="150.0" promptText="Pilih Lokasi" />
      <TextField fx:id="idPesananTF" layoutX="30.0" layoutY="293.0" />
      <Label layoutX="30.0" layoutY="266.0" text="Masukkan ID Teknisi" />
